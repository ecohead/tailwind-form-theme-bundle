{
    "name": "ecohead/symfony-tailwind-form-theme-bundle",
    "type": "symfony-bundle",
    "description": "Tailwind CSS v3 form theme for Symfony",
    "keywords": ["symfony-ux"],
    "license": "MIT",
    "authors": [
        {
            "name": "aureldvx",
            "email": "opensource@aureldvx.net"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "ext-intl": "*",
        "symfony/asset-mapper": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/form": "^7.0",
        "symfony/http-kernel": "^7.0",
        "symfony/stimulus-bundle": "2.x-dev"
    },
    "suggest": {
        "ecohead/symfony-form-generator-bundle": "To generate forms with type safety"
    },
    "autoload": {
        "psr-4": {
            "Ecohead\\TailwindFormThemeBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecohead\\TailwindFormThemeBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
